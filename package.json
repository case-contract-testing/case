{
  "name": "case",
  "version": "1.0.0",
  "description": "Contract testing suite",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "lint": "eslint src --ext .ts --config .eslintrc.json",
    "lint:fix": "eslint src --fix --ext .ts --config .eslintrc.json",
    "format:base": "prettier --parser typescript \"**/*.ts\"",
    "format:check": "npm run format:base -- --list-different",
    "format:fix": "npm run format:base -- --write ",
    "test": "jest"
  },
  "author": "Timothy Jones",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16-strictest": "^1.0.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.4.0",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "body-parser": "^1.20.2",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "absolute-version": "^1.0.1",
    "async-mutex": "^0.4.0",
    "axios": "^1.3.4",
    "chalk": "^4.1.2",
    "express": "^4.18.2",
    "filenamify": "^4.3.0",
    "pretty-format": "^29.4.1",
    "slug": "^8.2.2"
  }
}
